    "import socket\n",
    "server=socket.socket()\n",
    "ipaddr=('127.0.0.1',9998)\n",
    "server.bind(ipaddr)\n",
    "server.listen()\n",
    "s,raddr=server.accept()\n",
    "while True:\n",
    "    data=s.recv(1024)\n",
    "    print(data)\n",
    "    s.send('ack.()'.format(data.decode().encode))\n",
    "s.close()\n",
    "server.close\n",
    "\n",
    "\n",
    "\n"
   ]
